include ../subtitle/subtitle
include ../qualifications-item/qualifications-item

mixin experience({ degree, place, modifier, description, period })
  section.experience
    +subtitle("experience")

    .page__qualifications.page__qualifications_type_experience
      ul.experience__qualifications
        - for (var i = 0; i < 3; i += 1)
          .experience__qualification
            +qualifications-item({ degree, place, modifier, description, period })