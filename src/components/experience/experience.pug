include ../subtitle/subtitle
include ../qualifications/qualifications

mixin experience({ modifier, period, description, degree, place })
  section.experience
    +subtitle("experience")

    .page__qualifications.page__qualifications_type_experience
      ul.qualifications
        - for (var i = 0; i < 3; i += 1)
          +qualifications({ modifier, period, description, degree, place })